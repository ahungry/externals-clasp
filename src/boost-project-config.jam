# Boost.Build Configuration
# Automatically generated by bootstrap.sh

import option ;
import feature ;

# Compiler configuration. This definition will be used unless
# you already have defined some toolsets in your user-config.jam
# file.
if ! darwin in [ feature.values <toolset> ]
{
    using darwin ; 
}

project : default-build <toolset>darwin ;

# List of --with-<library> and --without-<library>
# options. If left empty, all libraries will be built.
# Options specified on the command line completely
# override this variable.
libraries =  --with-filesystem --with-date_time --with-serialization --with-iostreams --with-program_options --with-mpi --with-thread --with-regex --with-python ;

# These settings are equivivalent to corresponding command-line
# options.
option.set prefix : /Users/meister/Development/cando/build ;
option.set exec-prefix : /Users/meister/Development/cando/build ;
option.set libdir : /Users/meister/Development/cando/build/lib ;
option.set includedir : /Users/meister/Development/cando/build/include ;

# Stop on first error
option.set keep-going : false ;
